    <!-- Ajax Paging -->
<script type="text/javascript">

    var getPage = function () {
        var $a = $(this);
        var nexttarget = $a.parents().eq(5).attr("id");
        var disablednextlist = "next disabled PagedList-skipToNext";
        var disabledpreviouslist = "previous disabled PagedList-skipToPrevious";
        var currentitem = $(this).parent().attr('class');
        if (currentitem == disablednextlist) {
            return false;
        }
        else if (currentitem == disabledpreviouslist) {
            return false;
        } else {
            var options = {
                url: $a.attr("href"),
                data: $("form").serialize(),
                type: "get"
            };

            $.ajax(options).done(function (data) {
                var target = $a.parents().eq(5);
                $(target).empty().append($(data).find('#' + nexttarget));
            });
            return false;
        }
    };
    //Enable/Disable Ajax onPaging
    $(".main").on("click", ".pager li a", getPage);
    // $(".next disabled PagedList-skipToNext", ".previous disabled PagedList-skipToPrevious").off("click");
</script>
