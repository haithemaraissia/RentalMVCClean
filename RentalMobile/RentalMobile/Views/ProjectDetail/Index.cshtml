<section class="row">
    @section provider
    {
        <div id="agent" class="panel">
            <img src="../../images/dotimages/single-property/agent-480x350.png" alt="Agent" class="scale-with-grid">
        </div>
    }
    @section work
    {
        <div id="photos" class="panel">
            <a id="fancybox-manual-c" href="javascript:;">
                <img src="../../images/dotimages/single-property/gallery-480x350.png" class="img-preview scale-with-grid"
                    alt="image">
            </a>
        </div>
    }
    @section video
    {
        <div id="video" class="panel">
            <iframe src="http://player.vimeo.com/video/12158329" width="500" height="281" frameborder="0"
                webkitallowfullscreen mozallowfullscreen allowfullscreen></iframe>
        </div>
    }
    @section map
    {
        <div id="map" class="panel">
            <iframe width="100%" height="300" frameborder="0" scrolling="no" marginheight="0" marginwidth="0" src="http://maps.google.co.in/maps?q=@ViewBag.UnitGoogleMap&amp;z=13&amp;output=embed">
            </iframe>
            <br />
        </div>
    }
    @section providerrequestform
    {
        -1-FROM HOME - SHOW : BOX FOR YOUR NAME, EMAIl and HIRE CRWEW
        <h4>
            Recuest showing</h4>
        <p>
            Please complete the form below to request a showing for this property. Quisque non
            mi magna, eget aliquet urna. Curabitur ut justo est. Quisque iaculis risus et tortor
            congue porta. Suspendisse eleifend tempor dapibus.
        </p>
        @*    @using (Html.BeginForm("Select", "ChooseProvider", new { propertyIdcustom = 2, providerid = 1 }, FormMethod.Post))*@
        @using (Html.BeginForm("MakeOffer", "ProjectDetail", FormMethod.Post))
        {
            <ul class="grid-form">
                <li class="four columns alpha">
                     <label for="your-name" class="screen-reader-text">Your name</label>
                    @{
                        if (@ViewBag.TenantName != null)
                        {
                            <input id = "your-name" placeholder = "Your name" type = "text" required value = "@ViewBag.TenantName" style="text-align: center"/>
                        }
                        else
                        {
                            <input id="your-name1" placeholder="Your name" type="text" required style="text-align:center"/>
                        }
                    }
                </li> 
                <li class="four columns omega">
                     <label for="your-email" class="screen-reader-text">E-mail</label>
                    @{
                        if (@ViewBag.TenantEmail != null)
                        {
                            <input id = "your-email" placeholder = "email" type = "text" required value = "@ViewBag.TenantEmail" style="text-align: center"/>
                        }
                        else
                        {
                            <input id="your-email1" placeholder="email" type="text" required style="text-align: center"/>
                        }
                    }
                </li> 
                <li class="four columns alpha">
                    <label for="amountlabel" class="screen-reader-text">Amount </label>
                     <input id="amount" runat="server" placeholder="Amount"
                                             type="text" required style="text-align: center"/> 
                </li> 
                <li class="four columns omega">
                    <label for="currencylabel" class="screen-reader-text"> Currency</label> 
                    <input id="currency" runat="server" placeholder="Dollars" type="text" 
                           required style="text-align: center"/> 
                </li>
                <li class="four columns alpha">
                    <label for="enddate" class="screen-reader-text">Start date </label> <input id="startdatepicker" runat="server" placeholder="Start Date"
                                                                                         type="text" required style="text-align: center"/> 
                </li> 
                <li class="four columns omega">
                    <label for="enddate" class="screen-reader-text"> End date</label> 
                    <input id="enddatepicker" runat="server" placeholder="End Date" type="text" 
                           required style="text-align: center"/> 
                </li>
                
                  <li class="eight columns alpha">
                    <label for="quicknotelabel" class="screen-reader-text">Quick Note </label>
                     <input id="quicknote" runat="server" placeholder="Anything you like to mention"
                                             type="text" required style="text-align: center"/> 
                </li> 
            </ul>
            <input type="hidden" name="HiddenStartDate" id="HiddenStartDate" value="" />
            <input type="hidden" name="HiddenEndDate" id="HiddenEndDate" value="" />
        <input type="submit" class="full-width tall" value="Make an offer" id="MakeOffer"
        />
        }
    }
