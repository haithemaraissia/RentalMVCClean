@model RentalMobile.Models.RentalApplication
@{
    ViewBag.Title = "Submit Application";
    Layout = "~/Views/Shared/Template/_ConceptLayoutPage.cshtml";
}
<section class="sixteen columns">
    <h3 class="tagline">
        SELECT PROPERTY</h3>
</section>
<section class="sixteen columns">
    <h2>
        Advanced</h2>
    @using (Html.BeginForm("Register", "Account", new { @class = "boxed shadded wide-on-small-screen" }))
    {
        @Html.ValidationSummary(true, "Account creation was unsuccessful. Please correct the errors and try again.")
        
        <ul class="grid-form row remove-bottom">
            <li class="four columns alpha">
                <input id="prop-type-field" type="text" placeholder="Property ID" required />
            </li>
            <li class="two columns omega">
                <input type="submit" value="Select" />
            </li>
        </ul>
    }
</section>
<section class="sixteen columns">
    <h2>
        Scheduled</h2>
    <!-- filters -->
    <form action="#" class="row">
    <!-- tag filters -->
    <ul class="filter-list nine alpha columns row">
        <li><a data-rel="all" class="current">All</a></li>
        <li><a data-rel="house">House</a></li>
        <li><a data-rel="appartment">Apartment</a></li>
        <li><a data-rel="loft">Loft</a></li>
        <li><a data-rel="offices">Offices</a></li>
        <li><a data-rel="condos">Condos</a></li>
    </ul>
    <div class="select-filter five columns relative">
        <select id="sort-by" class="custom-select" title="Sort by">
            <option value="sort by">Sort by</option>
            <option value="1">Featured</option>
            <option value="2">Price Descending</option>
            <option value="3">Price Ascending</option>
            <option value="4">Date Descending</option>
            <option value="5">Date Ascending</option>
            <option value="6">Number of photos</option>
            <option value="7">Transtit</option>
            <option value="8">+ More Options</option>
        </select>
    </div>
    <div class="switcher fr">
        <a href="#" id="gridview" class="switcher"><i class="icon-layout"></i></a><a href="#"
            id="listview" class="switcher active"><i class="icon-menu"></i></a>
    </div>
    </form>
