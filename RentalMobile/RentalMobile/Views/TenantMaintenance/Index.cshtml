@model List<RentalMobile.Models.MaintenanceOrder>
@{
    ViewBag.Title = "Home Page";
    Layout = "~/Views/Shared/Template/_ConceptLayoutPage.cshtml";
}
<h2>
    Index</h2>
<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table>
    <tr>
        <th>
            Request ID
        </th>
        <th>
            UnitID
        </th>
        <th>
            Date
        </th>
        <th>
            Urgency
        </th>
        <th>
            Description
        </th>
        <th>
            Service
        </th>
        <th>
            Pets
        </th>
        <th>
            TenantPresence
        </th>
        <th>
        </th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.MaintenanceID)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.UnitID)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MaintenanceDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.UrgencyType.UrgencyType1)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Description)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ServiceType.ServiceType1)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PetsinUnit)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TenantPresence)
            </td>
            <td>
                @Html.Partial("_MaintenancePhotoGallery", item.MaintenancePhotoes)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { Id = item.MaintenanceID }) |
                @Html.ActionLink("Details", "Details", new { Id = item.MaintenanceID }) |
                @Html.ActionLink("Delete", "Delete", new { Id = item.MaintenanceID }) |
            </td>
        </tr>
    }
</table>
