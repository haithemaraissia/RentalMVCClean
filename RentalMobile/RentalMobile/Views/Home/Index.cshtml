<!-- Search form -->
<form action="#" class="sixteen columns shadded wide-on-small-screen">
<!-- tag filters -->
<ul class="radio-list fourteen columns push by-one">
    <li>
        <input type="radio" id="filter-1" name="operation-filter" value="1" checked />
        <label for="filter-1">
            For sale</label>
    </li>
    <li>
        <input type="radio" id="filter-2" name="operation-filter" value="2" />
        <label for="filter-2">
            For rent</label>
    </li>
    <li>
        <input type="radio" id="filter-3" name="operation-filter" value="3" />
        <label for="filter-3">
            Sold</label>
    </li>
    <li>
        <input type="radio" id="filter-4" name="operation-filter" value="4" />
        <label for="filter-4">
            Estimates</label>
    </li>
</ul>
<ul class="grid-form fourteen columns push by-one remove-bottom">
    <li class="eight columns alpha">
        <label for="location-field" class="screen-reader-text">
            Location</label>
        <input id="location-field" type="text" placeholder="Location" required />
    </li>
    <li class="three columns relative">
        <label for="beds-select" class="screen-reader-text">
            Beds</label>
        <select id="beds-select" class="custom-select" title="Beds">
            <option value="1">Beds</option>
            <option value="1">1</option>
            <option value="2">2</option>
            <option value="3">3</option>
            <option value="4">4</option>
            <option value="5">5</option>
        </select>
    </li>
    <li class="three columns omega relative">
        <label for="baths-select" class="screen-reader-text">
            Baths</label>
        <select id="baths-select" class="custom-select" title="Baths">
            <option value="1">Baths</option>
            <option value="1">1</option>
            <option value="2">2</option>
            <option value="3">3</option>
            <option value="4">4</option>
            <option value="5">5</option>
        </select>
    </li>
    <li class="three columns alpha">
        <label for="min-field" class="screen-reader-text">
            Min price</label>
        <input id="min-field" type="text" placeholder="$Min" required />
    </li>
    <li class="three columns">
        <label for="max-field" class="screen-reader-text">
            Max price</label>
        <input id="max-field" type="text" placeholder="$Max" required />
    </li>
    <li class="three columns">
        <label for="meters-field" class="screen-reader-text">
            Meters</label>
        <input id="meters-field" type="text" placeholder="Meters" required />
    </li>
    <li class="three columns">
        <label for="property-type-field" class="screen-reader-text">
            Property type</label>
        <input id="property-type-field" type="text" placeholder="Property type" required />
    </li>
    <li class="two columns omega">
        <input type="submit" value="Search" />
    </li>
</ul>
    <div class="fourteen columns push by-one add-bottom row">
@*        @Html.ActionLink("AdvancedSearch", "Index", "AdvancedSearch", new { @class = "fly-out"})*@
        <a href="AdvancedSearch" class="fl">Advanced
            Search</a><a href="#" class="fr">My Searches</a>
    </div>
</form>
